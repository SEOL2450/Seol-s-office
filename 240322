##import turtle
##
##t = turtle. Turtle()
##t.shape('turtle')
##
##r = 50
##t.pendown()
##t. circle(r)
##t.penup()
##t.goto(100,0)
##
##r = r + 30
##t.pendown()
##t. circle(r)
##t.penup()
##t.goto(200,0)
##
##r = r + 30
##t.pendown()
##t.circle(r)
##turtle.done

