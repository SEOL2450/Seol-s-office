##import turtle
##
##t = turtle.Turtle()
##
##t.setx(100)
##t.sety(100)
##t.setx(0)
##t.sety(0)
##
##t.clear()
##
##t.goto(100,0)
##t.goto(100,100)
##t.goto(0,100)
##t.goto(0,0)
##
##t.clear()
##
##t.setheading(120)
##t.forward(100)
##t.setheading(240)
##t.forward(100)
##t.home()
##
##turtle.done

